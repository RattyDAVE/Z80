            .ORG    $0000 
                    ;------------------------------------------------------------------------------
                    ; Reset

RST00:      RET      

                    ;------------------------------------------------------------------------------
                    ; TX a character over RS232

            .ORG    0008H 
RST08:      RET
                    ;------------------------------------------------------------------------------
                    ; RX a character over RS232 Channel A [Console], hold here until char ready.

            .ORG    0010H 
RST10:      RET 

                    ;------------------------------------------------------------------------------
                    ; Check serial status

            .ORG    0018H 
RST18:      RET 

                    ;------------------------------------------------------------------------------
                    ; RST 38 - INTERRUPT VECTOR [ for IM 1 ]

            .ORG    0038H 
RST38:      RET 

